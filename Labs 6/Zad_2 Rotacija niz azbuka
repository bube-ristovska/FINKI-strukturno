#include <stdio.h>
#define MAX 26
int main(){
    int k;
    char bukva;
    scanf("%c %d",&bukva, &k);
    char azbuka[MAX];
    for(int i = 0; i < 26; i++){
        azbuka[i]= i+'A';
    }

//naogjanje na mestoto na bukvata i rotacija za tolku mesta 

     for(int i = 0; i < 26; i++){
        if(azbuka[i] = bukva){
            printf("%c", azbuka[(bukva - 'A'+k)%26]); break;
        }
        break;
    }

return 0;
}
